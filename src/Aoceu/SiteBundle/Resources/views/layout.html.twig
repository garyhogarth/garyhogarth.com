{% extends '::base.html.twig' %}
{% block stylesheets %}
    {{ parent() }}

	<link rel="stylesheet/less" href="{{ asset('bundles/aoceusite/less/style.less') }}">
	<script src="{{ asset('bundles/aoceusite/js/libs/less-1.2.1.min.js') }}"></script>
	
	<!-- Use SimpLESS (Win/Linux/Mac) or LESS.app (Mac) to compile your .less files
	to style.css, and replace the 2 lines above by this one:

	<link rel="stylesheet/less" href="less/style.css">
	 -->
{% endblock %}
{% block title %}Gary Hogarth{% endblock %}
{% block body %}
     

    <div class="navbar navbar-fixed-top">
      <div class="navbar-inner">
        <div class="container">
          <a class="btn btn-navbar" data-toggle="collapse" data-target=".nav-collapse">
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
          </a>
          <a class="brand" href="#">Gary Hogarth</a>
          <div class="nav-collapse pull-right">
            {{ knp_menu_render('AoceuSiteBundle:Builder:mainMenu', {'currentClass': 'active','template' : 'AoceuSiteBundle:Partials:_main.menu.html.twig'}) }}
          </div><!--/.nav-collapse -->
        </div>
      </div>
    </div>

    <div class="container">
        {% block content %}
        {% endblock %}
      <hr>

      <footer>
        <p>&copy; AOCEU Ltd 2012</p>
      </footer>

    </div> <!-- /container -->
{% endblock %}